<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Provides deployment discovery services for identifying installed instances of Canonical Snap on a system.</Description>
    <PackageTags>$(PackageTags) deployment</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Using Include="Gapotchenko.FX" />
    <Using Include="Gapotchenko.FX.Diagnostics" />
    <Using Include="Gapotchenko.FX.IO" />
    <Using Include="Gapotchenko.FX.Linq" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Gapotchenko.FX" Version="2025.1.45" />
    <PackageReference Include="Gapotchenko.FX.Diagnostics.CommandShell" Version="2025.1.45" />
    <PackageReference Include="Gapotchenko.FX.IO" Version="2025.1.45" />
    <PackageReference Include="Gapotchenko.FX.Linq" Version="2025.1.45" />
  </ItemGroup>

</Project>
